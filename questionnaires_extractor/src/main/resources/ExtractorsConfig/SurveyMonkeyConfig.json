{
	"questionarios": [
		{
			"seletor": "",
			"assunto": {
				"seletor": "table.survey-title-table span.title-text",
				"metodo": "owntext"
			},
			"perguntas": {
				"seletor": "section.survey-page-body form[name=surveyForm] div.questions div.question-row",
				"descricao": {
					"seletor": "h4.question-title-container span.question-number, h4.question-title-container span.user-generated",
					"metodo": "text"
				},
				"alternativas": {
					"textarea": [
						{
							"seletor": "div.question-essay div.question-body textarea"
						}
					],
					"radio_input": [
						{
							"seletor": "div.question-single-choice-radio div.question-body div.answer-option-cell, div.question-single-choice-radio div.question-body div.other-answer-container",
							"descricao": {
								"seletor": "label.radio-button-label span.radio-button-label-text, label.other-answer-label",
								"metodo": "owntext"
							},
							"text_input": {
								"seletor": "input[type=text].other-answer-text"
							}
						}
					],
					"rating": [
						{
							"seletor": "table.question-matrix-nps-table tbody td.nps-radio-button-container",
							"descricao": {
								"seletor": "label.answer-label span.radio-button-label-text",
								"metodo": "owntext"
							}
						},
						{
							"seletor": "table.question-matrix-table tbody > tr > td",
							"descricao": {
								"seletor": "label.answer-label span.radio-button-label-text",
								"metodo": "owntext"
							}
						}
					],
					"checkbox_input": [
						{
							"seletor": "div.question-multiple-choice div.answer-option-cell div.checkbox-button-container, div.question-multiple-choice div.other-answer-container",
							"descricao": {
								"seletor": "label.checkbox-button-label span.checkbox-button-label-text",
								"metodo": "owntext"
							},
							"text_input": {
								"seletor": "input[type=text].other-answer-text"
							}
						}
					],
					"text_input": [
						{
							"seletor": "div.question-open-ended-single div.question-body > input[type=text].text"
						}
					],
					"range_input": [
						{
							"seletor": "div.question-open-ended-single div.slider-wrapper div.slider-question-wrapper",
							"valor_min": {
								"seletor": "table.slider-labels > tbody > tr > td:nth-child(1)",
								"metodo": "owntext"
							},
							"valor_max": {
								"seletor": "table.slider-labels > tbody > tr > td:nth-child(3)",
								"metodo": "owntext"
							}
						}
					],
					"select": [
						{
							"seletor": "div.question-single-choice-select div.question-body > select > option",
							"descricao": {
								"seletor": "",
								"metodo": "owntext"
							}
						}
					]
				}
			}
		}
	]
}
{
	"logLevels": "ERROR|INFO|DEBUG",
	"database": {
		"loadSeedsFromCrawler": false,
		"crawler": {
			"name": "tcc_crawler",
			"host": "localhost:5432",
			"login": "postgres",
			"password": "123"
		},
		"extractor": {
			"name": "tcc_extractor",
			"host": "localhost:5432",
			"login": "postgres",
			"password": "123"
		}
	},
	"crawler": {
		"numberOfCrawlers": 5,
		"crawlStorageFolder": "./tmp",
		"politenessDelay": 500,
		"maxDepthOfCrawling": 0,
		"maxPagesToFetch": -1,
		"includeBinaryContentInCrawling": false,
		"resumableCrawling": false	
	},
	"seeds": [
		"https://www.survio.com/modelo-de-pesquisa/feedback-sobre-servico",
		"https://www.survio.com/modelo-de-pesquisa/pesquisa-de-preco-do-produto",
		"https://www.survio.com/modelo-de-pesquisa/pesquisa-de-percepcao-da-publicidade-e-de-sua-eficiencia",
		"https://www.survio.com/modelo-de-pesquisa/pesquisa-da-notoriedade-da-marca",
		"https://www.survio.com/modelo-de-pesquisa/pesquisa-da-experiencia-de-compra"
	],
	"parameters": {
		"minQuestionsOnQuestionnaire": 2,
		"maxWordsInAGroupDescription": 5,
		"phrasesToIgnoreRegex": "^(((s|t)u )?nombre( completo)?:?|apellidos?:?|(seu|primeiro|(ú|u)ltimo) nome:?|sobrenome:?|(full|your|first|last) name:?|(get our )?newsletter|account id|subscribe|create (an )?account|contact us( now)?!?|enter your email.*|(send (a )?)?comments?:?|users?(name)?:?|sign( )?in.*|sign( )?up.*|register|login:?|log in here|password:?|forgot(ten)?( your)? password\\??|retrieve password|confirm password:?|keep me logged in|reset password|search:?|entre com seu email.*|comente:?|(envie ((um|seu) )?)?coment(a|á)rio:?|coment(á|a)rios?:?|usu(a|á)rio|entr(e|ar)|inscrev(a|er)-se|registr(ar|e-se)|logar|entrar|senha|repita (a|sua) senha:?|esqueceu sua senha\\?|me mantenha logado|mantenha-me logado|recupere sua senha|buscar:?)\\*?",
		"distBetweenNearNodes": {
			"height": 3,
			"maxHeight": 3,
			"width": 3
		},
		"distBetweenTextsInsideQuestionnaire": {
			"height": 4
		},
		"distBetweenCompAndText": {
			"height": 2,
			"maxHeight": 3
		},
		"distBetweenDescAndQuestion": {
			"height": 2,
			"maxHeight": 4
		},
		"distBetweenGroupAndFirstQuestion": {
			"height": 1,
			"width": 7
		},
		"distBetweenDescAndComplementaryText": {
			"height": 2,
			"maxHeight": 4,
			"width": 4
		},
		"distBetweenTextsInQuestionWithSubQuestions": {
			"height": 1,
			"width": 5
		},
		"distBetweenPartsOfDescription": {
			"width": 4,
			"height": 1,
			"maxHeight": 1
		},
		"distBetweenTextsOfSameAlternative": {
			"height": 1,
			"maxHeight": 2,
			"width": 2
		}
	}
}